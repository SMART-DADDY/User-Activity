{
    "name": "smart-daddy/user-activity",
    "description": "User activity tracking module for SMART-TiLL",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.4",
        "laravel/framework": "^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^10.0",
        "pestphp/pest": "^4.1",
        "pestphp/pest-plugin-laravel": "^4.0",
        "phpunit/phpunit": "^12.0"
    },
    "autoload": {
        "psr-4": {
            "SmartDaddy\\UserActivity\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\": "tests/Fixtures/App/",
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest"
    },
    "extra": {
        "laravel": {
            "providers": [
                "SmartDaddy\\UserActivity\\UserActivityServiceProvider"
            ]
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
